<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout_shoppingCartAllProduct"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Cart.CartAllProductFragment">

    <ScrollView
        android:id="@+id/scrollView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#F1F1F1"
        app:layout_constraintBottom_toTopOf="@+id/linaerLayout_footer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:id="@+id/LinearLayout_allproduct"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />
    </ScrollView>

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:background="@drawable/cart_view_color_8"
        app:layout_constraintBottom_toTopOf="@id/linaerLayout_footer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linaerLayout_footer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/RelativeLayout_cart_coupon"
            android:layout_width="match_parent"
            android:layout_height="40dp">


            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:alpha="0.6"
                android:src="@drawable/outline_local_activity_black_18"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView_greenpoint"
                android:layout_width="11dp"
                android:layout_height="11dp"
                android:layout_marginStart="-5dp"
                android:layout_marginBottom="-10dp"
                android:src="@drawable/greenpoint"
                android:visibility="gone"
                app:layout_constraintBottom_toTopOf="@id/imageView2"
                app:layout_constraintStart_toEndOf="@id/imageView_redpoint" />

            <ImageView
                android:id="@+id/imageView_redpoint"
                android:layout_width="11dp"
                android:layout_height="11dp"
                android:layout_marginStart="-8dp"
                android:layout_marginBottom="-10dp"
                android:src="@drawable/redpoint"
                android:visibility="gone"
                app:layout_constraintBottom_toTopOf="@id/imageView2"
                app:layout_constraintStart_toEndOf="@id/imageView2" />

            <TextView
                android:id="@+id/textView137"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="優惠券"
                android:textColor="@color/font_color"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toEndOf="@id/imageView2"
                app:layout_constraintTop_toTopOf="parent" />


            <TextView
                android:id="@+id/textView_cart_discount_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:background="@drawable/cart_allproduct_textview_border_discount"
                android:paddingHorizontal="3dp"
                android:paddingVertical="2dp"
                android:text="已選折價"
                android:textColor="@color/Mycolor_1"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView_cart_ship_select"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView_cart_ship_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:background="@drawable/cart_allproduct_textview_border_shipping"
                android:paddingHorizontal="3dp"
                android:paddingVertical="2dp"
                android:text="已選免運"
                android:textColor="#00B5A5"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView_cart_no_select"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView_cart_no_select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="5dp"
                android:text="選擇優惠券"
                android:textColor="#8E8E8E"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/imageView10"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="4dp"
                android:alpha="0.5"
                android:src="@drawable/outline_keyboard_arrow_up_black_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView_cart_total"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center"
                android:paddingEnd="8dp"
                android:text="168"
                android:textColor="@color/Mycolor_1"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@id/textView_cart_checkout" />

            <TextView
                android:id="@+id/textView_cart_checkout"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:background="@color/Mycolor_1"
                android:gravity="center"
                android:paddingHorizontal="24dp"
                android:text="去買單 (0)"
                android:textColor="@color/white"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/textView_cart_1"
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="$"
                android:textColor="@color/Mycolor_1"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView_cart_total" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="50dp"
                android:gravity="center"
                android:paddingEnd="8dp"
                android:text="總金額"
                android:textColor="@color/font_color"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView_cart_1" />

            <CheckBox
                android:id="@+id/checkBox_cart_selectAll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:paddingHorizontal="6dp"
                android:text="全選"
                android:textColor="@color/font_color"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>